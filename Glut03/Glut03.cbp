<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Glut03" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Glut03" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#glut)/bin" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Glut03" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#glut)/bin" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#glut.include)" />
		</Compiler>
		<Linker>
			<Add library="glut32" />
			<Add library="opengl32" />
			<Add library="glu32" />
			<Add library="winmm" />
			<Add library="gdi32" />
			<Add directory="$(#glut.lib)" />
		</Linker>
		<Unit filename="GLee.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="GLee.h" />
		<Unit filename="glframe.h" />
		<Unit filename="gltools.cpp" />
		<Unit filename="gltools.h" />
		<Unit filename="math3d.cpp" />
		<Unit filename="math3d.h" />
		<Unit filename="sphereworld.cpp" />
		<Unit filename="wglext.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
